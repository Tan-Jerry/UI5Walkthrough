<mvc:View
	controllerName="sap.ui.learning.controller.Detail"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:lc="sap.ui.learning.control">
	<Page
		title="{i18n>detailPageTitle}"
		showNavButton="true"
		navButtonPress=".onNavBack">
		<ObjectHeader
			intro="{invoice>ShipperName}"
			title="{invoice>ProductName}"/>
		<lc:ProductRating id="rating" class="sapUiSmallMarginBeginEnd" change=".onRatingChange" />
	</Page>
</mvc:View>